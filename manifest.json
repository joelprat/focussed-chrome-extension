{
  "manifest_version": 3,
  "name": "Focussed",
  "version": "1.1",
  "permissions": ["declarativeNetRequest", "declarativeNetRequestFeedback", "storage", "alarms", "notifications"],
  "host_permissions": ["<all_urls>"],
  "background": { "service_worker": "./src/background.js" },
  "action": { "default_popup": "./src/popup.html" },
  "icons": {
    "16": "icons/icon50.png",
    "48": "icons/icon50.png",
    "128": "icons/icon150.png"
  }
}
